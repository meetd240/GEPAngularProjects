{
  "name": "requisition-ng5",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test --code-coverage --single-run --browsers=ChromeHeadless --reporters=junit,progress",
    "test1": "ng test --code-coverage --reporters=junit,progress",
    "test2": "ng test --code-coverage --single-run --log-level=DEBUG --reporters=junit,progress",
    "debugKarma": "karma start ./configs/karma.conf.js --browsers=Chrome --single-run=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "refreshAuth": "vsts-npm-auth -config .npmrc",
    "clean": "rimraf ./distribution/__buildVersion___reqng5",
    "cache:verify": "npm cache verify",
    "live:server": "live-server distribution/__buildVersion___reqng5 --cors",
    "startLocal": "webpack-dev-server --config configs/build_conf/webpack.local.js --inline --progress --profile --port 3000",
    "build:local": "webpack-dev-server --config configs/build_conf/webpack.local.js --inline --progress --profile --port 3000",
    "buildLocal": "webpack-dev-server --config configs/build_conf/webpack.local.js --inline --progress --profile --port 3000",
    "build:dev": "webpack --progress --profile --bail --config configs/build_conf/webpack.dev.js",
    "build:devWatch": "webpack --progress --profile --bail --config configs/build_conf/webpack.dev.js --watch",
    "build:release": "webpack --progress --profile --bail --config configs/build_conf/webpack.release.js --optimize-minimize",
    "live:build:dev": "npm run clean && concurrently --kill-others \"npm run build:dev\" \"npm run live:server\"",
    "live:build:release": "npm run clean && concurrently --kill-others \"npm run build:release\" \"npm run live:server\"",
    "build:prod1": "npm run clean && concurrently --kill-others \"npm run build:dev\" \"npm run build:release\"",
    "buildProd": "npm run clean && npm run build:dev && npm run cache:verify && npm run build:release --max-old-space-size=10240"
  },
