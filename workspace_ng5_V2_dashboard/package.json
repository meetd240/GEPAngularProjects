    "e2e": "ng e2e",
    "refreshAuth": "vsts-npm-auth -config webpackConfig/.npmrc",
    "clean": "rimraf ../distribution/__buildVersion___dashboard__V2",
    "live:server": "live-server distribution/__buildVersion___dashboard__V2",
    "build:local": "webpack-dev-server --max_old_space_size=8192 --config  webpackConfig/buildConfiguration/webpack.local.js --inline --progress --profile --port 3333",
    "build:dev": "webpack --progress --profile --bail --max_old_space_size=8192 --config webpackConfig/buildConfiguration/webpack.dev.js",
    "build:release": "webpack --progress --profile --bail --config webpackConfig/buildConfiguration/webpack.prod.js --optimize-minimize",
    "live:build:dev": "npm run clean && concurrently --kill-others \"npm run build:dev\" \"npm run live:server\"",
    "live:build:release": "npm run clean && concurrently --kill-others \"npm run build:release\" \"npm run live:server\"",
    "build:prod": "npm run clean && npm run build:dev && npm run build:release",
    "buildDV2": "npm run clean && npm run build:dev"
